<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context="com.yunyouzhiyuan.qianbaoshangcheng.activity.KTVPriceActivity">

    <include layout="@layout/top" />

    <com.yunyouzhiyuan.qianbaoshangcheng.ui.ScrollExpandableListView
        android:id="@+id/ktvprice_listview"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />

    <Button
        android:onClick="subMit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/app_color"
        android:minHeight="@dimen/pading_6"
        android:padding="@dimen/pading_10"
        android:text="@string/ok"
        android:textColor="@color/white"
        android:textSize="@dimen/size_text_14" />

</LinearLayout>
