<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape>
            <solid android:color="#ffffff"/>
            <stroke android:color="#f1d9d9"  android:width="1dp"/>
            <padding
                android:bottom="6dp"
                android:left="14dp"
                android:right="14dp"
                android:top="6dp"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape>
            <solid android:color="#f5e8ba"/>
            <stroke android:color="#fd7803"  android:width="1dp"/>
            <padding
                android:bottom="6dp"
                android:left="14dp"
                android:right="14dp"
                android:top="6dp"/>
        </shape>
    </item>


</selector>